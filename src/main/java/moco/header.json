[
  {
    "description": "模拟一个带header的post请求:HTTP请求-postwithheads",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "headers": {
        "accept": "application/xml;application/json;charset=utf-8",
        "content-type": "application/json"
      },
      "json": {
        "name": "Tom",
        "sex": "man"
      }
    },
    "response": {
      "json": {
        "Tom": "success",
        "status": "1"
      }
    }
  }
]